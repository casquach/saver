{
    "name": "Stackoverflow Popup Example",
    "version": "0.1",
    "description": "Run process on page activated by click in extension popup",
    "background": {
        "scripts": ["background.js"],
          "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": ["tabs", "background"],
    "manifest_version": 2
}
